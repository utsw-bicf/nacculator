            "type": "string",
            "linkTo": "Lab",
            "linkSubmitsFor": true
        },
        "award": {
            "title": "Award",
            "description": "Grant associated with the submission.",
            "comment": "Required. See award.json for list of available identifiers.",
            "type": "string",
            "linkTo": "Award"
        }
    },
    "attachment": {
        "attachment": {
            "title": "Attachment",
            "type": "object",
            "additionalProperties": false,
            "formInput": "file",
            "attachment": true,
            "properties": {
                "download": {
                    "title": "File Name",
                    "type": "string"
                },
                "href": {
                    "comment": "Internal webapp URL for document file",
                    "type": "string"
                },
                "type": {
                    "title": "MIME type",
                    "type": "string",
                    "enum": [
                        "application/pdf",
                        "image/gif",
                        "image/jpeg",
                        "image/png",
                        "image/svs",
                        "image/tiff",
                        "text/autosql",
                        "text/html",
                        "text/plain",
                        "text/tab-separated-values"
