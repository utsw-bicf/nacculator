                },
                "md5sum": {
                    "title": "MD5sum",
                    "type": "string",
                    "format": "md5sum"
                },
                "size": {
                    "title": "File size",
                    "type": "integer",
                    "minimum": 0
                },
                "width": {
                    "title": "Image width",
                    "type": "integer",
                    "minimum": 0
                },
                "height": {
                    "title": "Image height",
                    "type": "integer",
                    "minimum": 0
                }
            }
        }
